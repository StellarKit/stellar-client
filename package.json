{
  "name": "stellar-client",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Test application for the StellarSdk",
  "repository": "github.com/StellarKit/stellar-client",
  "homepage": "https://stellarkit.io",
  "author": {
    "name": "Steve Gehrman",
    "email": "steve@distantfutu.re"
  },
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "asar": true,
    "productName": "Stellar Client",
    "appId": "io.stellarkit.stellar-client",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "createDesktopShortcut": false
    }
  },
  "dependencies": {
    "electron-context-menu": "^0.9.1",
    "jquery": "^3.3.1",
    "localforage": "^1.5.6",
    "md5": "^2.2.1",
    "nconf": "^0.10.0",
    "request": "^2.83.0",
    "sillyname": "^0.1.0",
    "source-map-support": "^0.5.3",
    "stellar-js-utils": "git://github.com/StellarKit/stellar-js-utils",
    "stellar-ledger-api": "^2.0.4",
    "stellar-sdk": "^0.8.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "electron": "1.7.11",
    "electron-builder": "^19.55.3",
    "electron-webpack": "1.12.1",
    "electron-webpack-eslint": "^1.3.0",
    "electron-webpack-vue": "^1.3.0",
    "eslint": "^4.16.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-vue": "4.2.2",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "vue": "^2.5.13",
    "vue-loader": "^14.0.1",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.13",
    "vuetify": "^0.17.6",
    "webpack": "^3.10.0",
    "webpack-build-notifier": "^0.1.22"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  }
}
