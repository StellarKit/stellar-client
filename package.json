{
  "name": "stellar-client",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Test application for the StellarSdk",
  "repository": "github.com/StellarKit/stellar-client",
  "homepage": "https://stellarkit.io",
  "author": {
    "name": "Steve Gehrman",
    "email": "steve@distantfutu.re"
  },
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "electron-webpack && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "asar": true,
    "productName": "Stellar Client",
    "appId": "io.stellarkit.stellar-client",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "createDesktopShortcut": false
    }
  },
  "dependencies": {
    "electron-context-menu": "^0.9.1",
    "jquery": "^3.3.1",
    "md5": "^2.2.1",
    "nconf": "^0.10.0",
    "source-map-support": "^0.5.4",
    "stellar-client-web": "git://github.com/StellarKit/stellarkit-client-web",
    "@ledgerhq/hw-transport-node-hid": "^4.7.6",
    "babel-runtime": "6.26.0"
  },
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "electron-webpack": "^2.0.1",
    "electron-webpack-eslint": "^2.1.0",
    "electron-webpack-vue": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-vue": "^4.4.0",
    "webpack": "^4.5.0",
    "webpack-build-notifier": "^0.1.23"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  }
}
